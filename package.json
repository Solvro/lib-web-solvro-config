{
  "name": "@solvro/config",
  "version": "1.5.2",
  "description": "Solvro's engineering style guide",
  "keywords": [
    "eslint"
  ],
  "homepage": "https://github.com/Solvro/lib-web-solvro-config#readme",
  "bugs": {
    "url": "https://github.com/Solvro/lib-web-solvro-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Solvro/lib-web-solvro-config.git"
  },
  "license": "MPL-2.0",
  "type": "module",
  "exports": {
    "./eslint": {
      "types": "./dist/eslint/index.d.ts",
      "import": "./dist/eslint/index.js",
      "require": "./dist/eslint/index.cjs"
    },
    "./prettier": {
      "types": "./dist/prettier/index.d.ts",
      "import": "./dist/prettier/index.js",
      "require": "./dist/prettier/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "npx @eslint/config-inspector",
    "format": "prettier --check .",
    "lint": "eslint .",
    "prepare": "husky",
    "prepublish": "npm run build",
    "release": "semantic-release",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*": "prettier -w --ignore-unknown"
  },
  "prettier": "./dist/prettier/index.js",
  "dependencies": {
    "@adonisjs/eslint-config": "^2.0.0-beta.7",
    "@antfu/install-pkg": "^0.5.0",
    "@clack/prompts": "^0.8.2",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint-react/eslint-plugin": "^1.17.3",
    "@eslint/js": "^9.16.0",
    "@eslint/markdown": "^6.2.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "@vitest/eslint-plugin": "^1.1.12",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-flat-config-utils": "^0.4.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-antfu": "^2.7.0",
    "eslint-plugin-command": "^0.2.6",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-x": "^4.4.3",
    "eslint-plugin-jsdoc": "^50.6.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.14.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^4.1.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "find-up-simple": "^1.0.0",
    "globals": "^15.12.0",
    "local-pkg": "^0.5.1",
    "picocolors": "^1.1.1",
    "prettier-plugin-packagejson": "^2.5.6",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "typescript-eslint": "^8.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/config-inspector": "^0.5.6",
    "@next/eslint-plugin-next": "^15.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.10.1",
    "@types/yargs": "^17.0.33",
    "eslint": "^9.16.0",
    "eslint-typegen": "^0.3.2",
    "husky": "^9.1.7",
    "jiti": "^2.4.1",
    "lint-staged": "^15.2.10",
    "parse-gitignore": "^2.0.0",
    "prettier": "^3.4.1",
    "react": "^18.3.1",
    "semantic-release": "^24.2.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "yargs": "^17.7.2"
  },
  "peerDependencies": {
    "@next/eslint-plugin-next": ">=12.3.0 <16.0.0-0",
    "eslint": "^9.10.0",
    "prettier": ">=3.0.0 <4",
    "typescript": ">=4.8.0 <6"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    }
  },
  "packageManager": "npm@10.9.1",
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  }
}
